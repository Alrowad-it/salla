<section class="s-block s-block--categories">
<div class="container">
  <salla-slider type="carousel" show-controls="false" block-title="{{component.categories_title}}" >
  <div slot="items">
    {% for cat in component.category_item %}
    <div class="swiper-slide slide--one-sixth">
    <a href="#" class="slide--cat-entry" data-emergence="hidden">
    <img class="bg-cover mb-8 "  src="{{cat.image}}"/>

    <h4>{{cat.categorytitle}}</h4>

    </a>
    </div>
    {% endfor %}
  </div>


  </salla-slider>

</div>

</section>


